{
  "name": "breeze",
  "version": "2.2.64",
  "description": "Fully automated API and Load Testing",
  "main": "breeze.js",
  "scripts": {
    "dev": "nodemon",
    
    "test": "NODE_ENV=unittest node_modules/mocha/bin/mocha --reporter mochawesome --exit --reporter-options reportDir=test/mochawesome-report,reportFilename=${npm_package_name},reportTitle=${npm_package_name},reportPageTitle=${npm_package_name},json=false,inline=true,code=false"
  },
  "keywords": [
    "meta",
    "node",
    "es6"
  ],
  "author": "NaG",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "2.4.1",
    "@google-cloud/trace-agent": "3.5.2",
    "@grpc/proto-loader": "0.3.0",
    "ajv": "^5.0.0",
    "amqp-connection-manager": "2.3.0",
    "amqplib": "0.5.3",
    "async": "2.6.1",
    "aws-sdk": "2.437.0",
    "body-parser": "1.18.3",
    "chai": "4.1.2",
    "colors": "1.3.0",
    "commander": "2.17.1",
    "crypto": "1.0.1",
    "db-migrate": "0.11.1",
    "db-migrate-mysql": "1.1.10",
    "env2": "2.2.2",
    "express": "4.16.3",
    "faker": "4.1.0",
    "grpc": "1.13.1",
    "inquirer": "6.2.1",
    "ioredis": "4.9.1",
    "moment": "2.23.0",
    "moment-timezone": "0.5.23",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "microtime": "2.1.8",
    "mocha": "5.2.0",
    "mochawesome": "^4.1.0",
    "morgan": "^1.9.1",
    "mysql": "2.16.0",
    "nats": "1.0.1",
    "prom-client": "11.2.1",
    "protobufjs": "6.8.6",
    "protocol-buffers-encodings": "1.1.0",
    "request": "2.88.0",
    "rewire": "4.0.1",
    "sinon": "6.1.4",
    "sinon-chai": "3.2.0",
    "sqs-consumer": "5.2.0",
    "util": "0.11.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-google": "0.9.1",
    "nodemon": "1.18.3"
  },
  "nodemonConfig": {
    "ext": "js, proto",
    "ignore": [
      "*test.js",
      ".git/",
      "node_modules/",
      "test/",
      ".vscode"
    ],
    "delay": "3"
  },
  "private": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "bit bucker repo url"
  },
  "homepage": "/bitbucket#readme"
}